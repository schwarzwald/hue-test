{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Server",
      "request": "launch",
      "runtimeArgs": [
        "start"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Watch Sass",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "sass"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}